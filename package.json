{
  "name": "overtake",
  "version": "1.3.2",
  "description": "NodeJS performance benchmark",
  "type": "module",
  "types": "build/index.d.ts",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.cjs",
    "import": "./build/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/index.d.ts"
      ]
    }
  },
  "bin": {
    "overtake": "bin/overtake.js"
  },
  "scripts": {
    "build": "rm -rf build && inop src build -i __tests__ -i *.tmp.ts && tsc --declaration --emitDeclarationOnly",
    "start": "./bin/overtake.js",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/overtake.git"
  },
  "keywords": [
    "benchmark",
    "performance",
    "measure",
    "worker"
  ],
  "author": "Ivan Zakharchanka",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/3axap4eHko/overtake/issues"
  },
  "homepage": "https://github.com/3axap4eHko/overtake#readme",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@swc/jest": "^0.2.39",
    "@types/async": "^3.2.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@types/progress": "^2.0.7",
    "husky": "^9.1.7",
    "inop": "^0.9.0",
    "jest": "^30.2.0",
    "overtake": "^1.3.1",
    "prettier": "^3.8.1",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@swc/core": "^1.15.11",
    "async": "^3.2.6",
    "commander": "^14.0.3",
    "glob": "^13.0.6",
    "progress": "^2.0.3"
  },
  "packageManager": "pnpm@10.30.1+sha512.3590e550d5384caa39bd5c7c739f72270234b2f6059e13018f975c313b1eb9fefcc09714048765d4d9efe961382c312e624572c0420762bdc5d5940cdf9be73a"
}
