{
  "name": "overtake",
  "version": "1.0.3",
  "description": "NodeJS performance benchmark",
  "type": "module",
  "types": "build/index.d.ts",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.cjs",
    "import": "./build/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/index.d.ts"
      ]
    }
  },
  "bin": {
    "overtake": "bin/overtake.js"
  },
  "scripts": {
    "build": "rm -rf build && inop src build -i __tests__ -i *.tmp.ts && tsc --declaration --emitDeclarationOnly",
    "start": "./bin/overtake.js",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3axap4eHko/overtake.git"
  },
  "keywords": [
    "benchmark",
    "performance",
    "measure",
    "worker"
  ],
  "author": "Ivan Zakharchanka",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/3axap4eHko/overtake/issues"
  },
  "homepage": "https://github.com/3axap4eHko/overtake#readme",
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@swc/jest": "^0.2.39",
    "@types/async": "^3.2.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "husky": "^9.1.7",
    "inop": "^0.7.9",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@swc/core": "^1.13.5",
    "async": "^3.2.6",
    "commander": "^14.0.0",
    "glob": "^11.0.3"
  }
}
