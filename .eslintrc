{
  "$schema": "https://json.schemastore.org/eslintrc.json",
  "env": {
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["prettier", "eslint-config-airbnb-base"],
  "rules": {
    "import/extensions": 0,
    "no-plusplus": 0,
    "no-await-in-loop": 0,
    "no-shadow": 0,
    "no-new-func": 0,
    "object-curly-newline": [
      2,
      {
        "ObjectExpression": {
          "consistent": true,
          "multiline": true,
          "minProperties": 8
        },
        "ObjectPattern": {
          "consistent": true,
          "multiline": true
        },
        "ImportDeclaration": "never",
        "ExportDeclaration": {
          "multiline": true,
          "minProperties": 8
        }
      }
    ]
  }
}
